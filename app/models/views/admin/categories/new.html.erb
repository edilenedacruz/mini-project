<h1>Type one word to create a category</h1>

<%= form_for [:admin, @category] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.submit "Create a Category", class: "button" %>
<% end %>

<%= button_to "Return to Admin user page", user_path(current_user), class: "button", method: :get %>
